	ft_putstr("Hello World!\n");

ex05-expected:
Hello World!

ex06:

	int i = 0;
	i = ft_strlen("Hello World!\n");
	printf("len : %d", i);

ex06-expected:
len : 13

ex07:

	int test[9] = {1, 2, 3, 4, 5, 6, 7, 8, 9};

	ft_rev_int_tab(test, 9);
	for (int i = 0; i < 9; i++)
	{
		printf("%d, ", test[i]);
	}
	printf("\n");
	ft_rev_int_tab(test, 3);
	for (int i = 0; i < 9; i++)
	{
		printf("%d, ", test[i]);
	}
	printf("\n");

ex07-expected:
9, 8, 7, 6, 5, 4, 3, 2, 1,
7, 8, 9, 6, 5, 4, 3, 2, 1,

ex08:

	int test[9] = {1, 6, 3, 8, 4, 2, 9, 7, 5};
	ft_sort_int_tab(test, 9);
	for (int i = 0; i < 9; i++) {
		printf("%d, ", test[i]);
	}
	printf("\n");


ex08-expected:
1, 2, 3, 4, 5, 6, 7, 8, 9,

