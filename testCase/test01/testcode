ex00:

	int i = 1423;
	ft_ft(&i);
	printf("%d\n", i);

ex00-expected:
42



ex01:

	int i = 1423;
	int *i1 = &i;
	int **i2 = &i1;
	int ***i3 = &i2;
	int ****i4 = &i3;
	int *****i5 = &i4;
	int ******i6 = &i5;
	int *******i7 = &i6;
	int ********i8 = &i7;
	int *********i9 = &i8;
	ft_ultimate_ft(i9);
	printf("%d\n", i);

ex01-expected:
42


ex02:

	int i = 3;
	int j = 5;
	ft_swap(&i, &j);
	printf("i, j: %d, %d", i, j);

ex02-expected:
i, j: 5, 3


ex03:

	int div = 0, mod = 0;
	ft_div_mod(473, 25, &div, &mod);
	printf("div, mod : %d, %d\n", div, mod);
	ft_div_mod(-43, 3, &div, &mod);
	printf("div, mod : %d, %d\n", div, mod);
	ft_div_mod(0, 5, &div, &mod);
	printf("div, mod : %d, %d\n", div, mod);

ex03-expected:
div, mod : 18, 23
div, mod : -14, -1
div, mod : 0, 0

ex04:

	int a = 473, b = 25;
	ft_ultimate_div_mod(&a, &b);
	printf("a, b : %d, %d\n", a, b);
	a = -43, b = 3;
	ft_ultimate_div_mod(&a, &b);
	printf("a, b : %d, %d\n", a, b);
	a = 0, b = 5;
	ft_ultimate_div_mod(&a, &b);
	printf("a, b : %d, %d\n", a, b);

ex04-expected:
a, b : 18, 23
a, b : -14, -1
a, b : 0, 0

